<p id="notice"><%= notice %></p>

<h1>Work Sessions</h1>

<table>
  <thead>
    <tr>
      <th>Start</th>
      <th>End</th>
      <th>Pause</th>
      <th>Keys</th>
      <th>Backspace</th>
      <th>Last key</th>
      <th>Task</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @work_sessions.each do |work_session| %>
      <tr>
        <td><%= work_session.start %></td>
        <td><%= work_session.end %></td>
        <td><%= work_session.pause %></td>
        <td><%= work_session.keys %></td>
        <td><%= work_session.backspace %></td>
        <td><%= work_session.last_key %></td>
        <td><%= work_session.task_id %></td>
        <td><%= link_to 'Show', work_session %></td>
        <td><%= link_to 'Edit', edit_work_session_path(work_session) %></td>
        <td><%= link_to 'Destroy', work_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Work Session', new_work_session_path %>
